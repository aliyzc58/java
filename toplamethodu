
package dizitopla;
import java.util.Scanner;

public class DiziTopla {

    public static int topla(int a, int b) {
int sonuc= a+b;
return(sonuc);
}


    public static void main(String[] args) {
       
      int x=6;
      int y=9;
      int sonuc = topla(x,y);
      System.out.println(sonuc);
        
    }
    
    
}
