import java.util.Scanner;
public class Main
{
  public static void main (String[]args)
  {
    //Scanner giris = new Scanner (System.in);

    int a[] = { 1, 2, 3, 4, 5 };
    int n = a.length;
    int b[] = new int[n];

  /*  for (int i = 0; i < n; i++)
      System.out.print (a[i] + " ");*/
    for (int i = 0; i < n; i++)
      b[i] = a[n - i - 1];
    System.out.println ();
    for (int i = 0; i < n; i++)
      System.out.print (b[i] + " ");






  }
}
