package merhaba;

/**
 *
 * @author Beykent
 */
public class Merhaba {

    public static int ustAl(int x, int y)
{
int sonuc=1;
for(int i=1;i<=y;i++)
sonuc*=x;
return sonuc;
}
    public static int faktoriel (int x)
    {
        if(x==0||x==1)
            return 1;
        else
        {
            int fakt=1;
        for(int i=1;i<=x;i++)
            fakt=fakt*i;
        return (fakt);
        
        }
        
    }

    public static void main(String[] args) {


System.out.println(faktoriel(5));
    }
    
}
